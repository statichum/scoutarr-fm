##### DRY RUN #####

recommender:
  dry_run: true # outputs a list of artists to add without adding to Lidarr

##### MUSICBRAINZ CONFIG #####

musicbrainz:
  musicbrainz_url: "https://musicbrainz.org" # Default https://musicbrainz.org but use self hosted instance if you have it!

##### LISTENBRAINZ CONFIG ####

listenbrainz:
  user_token: "387df6f3-466xxxxxxxxxxxxxx" # from here https://listenbrainz.org/settings/
  username: "your-listenbrainz-username"   # yeah...

  # Recommend lists
  collaborative-filtering: true  # Suggestions based on your plays vs comparable users
  weekly-exploration: true       # Uses the Weekly Exploration playlist (includes diversity suggestions)

##### PLEX CONFIG ####

plex:
  enabled: true  # Enables import of previous weeks' Weekly Exploration playlist into Plex
  plex-token: "2tsMJxxxxxxxxxxxx"  # X-Plex token
  plex-username: "your-plex-username"
  plex-url: "http://100.121.111.8:32400"  # Must be accessible by docker container
  plex-library: "Music"  # Library name where the playlist will be created

  # Playlists
  pl-retention: 6            # Number of weeks of playlists to keep
  pl-name: "ListenBrainz Weekly Explore"  # Prefix, followed by ' â€“ YYYY Www'

##### LIDARR CONFIG ####

lidarr:
  enabled: true  # Enables adding artists to Lidarr
  url: "http://100.121.111.8:8686"  # Must be accessible by docker container
  api_key: "e927b0647cxxxxxxxxxxxxxxx"  # Lidarr > Settings > General

  root_folder: "/mnt/Music"  # Root folder to be used in Lidarr

  quality_profile: "Any"     # Quality profile name from Lidarr
  metadata_profile: "Album"  # Metadata profile name from Lidarr

  monitor_existing: "all"    # From Lidarr UI: Monitor
  monitor_new: "all"         # From Lidarr UI: Monitor New Albums
  search_on_add: true        # From Lidarr UI: Start search for missing albums

  tags:
    - "scoutarr-fm"  # Tag to be entered in Lidarr
